{
    "name": "fsac/laravel-windicator",
    "description": "Tailwind responsive screen indicator",
    "keywords": [
        "laravel",
        "tailwind",
        "tailwindcss",
        "responsive"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joby Harding",
            "email": "code@fullstackapp.co",
            "homepage": "https://fullstackapp.co",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FullStackAppCo\\Windicator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FullStackAppCo\\Windicator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FullStackAppCo\\Windicator\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.22"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
